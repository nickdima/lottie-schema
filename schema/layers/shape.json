{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ty": {
      "title": "Type",
      "description": "图层类型：形状。",
      "type": "number",
      "const": 4
    },
    "ks": {
      "title": "Transform",
      "description": "转换属性",
      "oneOf": [
        {
          "$ref": "helpers/transform.json"
        }
      ],
      "type": "object"
    },
    "ao": {
      "title": "Auto-Orient",
      "description": "沿路径AE属性自动定向。",
      "oneOf": [
        {
          "$ref": "helpers/boolean.json"
        }
      ],
      "type": "number",
      "default": 0
    },
    "bm": {
      "title": "Blend Mode",
      "description": "混合模式",
      "oneOf": [
        {
          "$ref": "helpers/blendMode.json"
        }
      ],
      "type": "number",
      "default": 0
    },
    "ddd": {
      "title": "3d Layer",
      "description": "3d图层标志",
      "oneOf": [
        {
          "$ref": "helpers/boolean.json"
        }
      ],
      "type": "number",
      "default": 0
    },
    "ind": {
      "title": "Index",
      "description": "AE中的层索引。用于父母和表达式。",
      "type": "number"
    },
    "cl": {
      "title": "Class",
      "description": "在SVG / HTML渲染器上用作html类的解析图层名称",
      "type": "string"
    },
    "ln": {
      "title": "layer HTML ID",
      "description": "在SVG / HTML渲染器上用作html id的解析图层名称",
      "type": "string"
    },
    "ip": {
      "title": "In Point",
      "description": "初始帧",
      "type": "number"
    },
    "op": {
      "title": "Out Point",
      "description": "结束帧",
      "type": "number"
    },
    "st": {
      "title": "Start Time",
      "description": "图层的开始时间。设置图层的开始时间。",
      "type": "number"
    },
    "nm": {
      "title": "Name",
      "description": "After Effects图层名称。用于KeyPath。",
      "type": "number"
    },
    "hasMask": {
      "title": "Has Masks",
      "description": "图层具有蒙版时的布尔值。将弃用以支持检查maskProperties。",
      "type": "number"
    },
    "maskProperties": {
      "title": "Mask Properties",
      "description": "面具清单",
      "items": {
        "oneOf": [
          {
            "$ref": "helpers/mask.json"
          }
        ],
        "type": "object"
      },
      "type": "array"
    },
    "ef": {
      "title": "Effects",
      "description": "效果列表",
      "items": {
        "oneOf": [
          {
            "$ref": "effects/index.json"
          }
        ],
        "type": "object"
      },
      "type": "array"
    },
    "sr": {
      "title": "Stretch",
      "description": "图层时间拉伸",
      "type": "number",
      "default": 1
    },
    "parent": {
      "title": "Parent",
      "description": "图层父级。使用父母的ind。",
      "type": "number"
    },
    "shapes": {
      "title": "shapes",
      "description": "shapes",
      "items": {
        "oneOf": [
          {
            "$ref": "shapes/shape.json"
          },
          {
            "$ref": "shapes/rect.json"
          },
          {
            "$ref": "shapes/ellipse.json"
          },
          {
            "$ref": "shapes/star.json"
          },
          {
            "$ref": "shapes/fill.json"
          },
          {
            "$ref": "shapes/gFill.json"
          },
          {
            "$ref": "shapes/gStroke.json"
          },
          {
            "$ref": "shapes/stroke.json"
          },
          {
            "$ref": "shapes/merge.json"
          },
          {
            "$ref": "shapes/trim.json"
          },
          {
            "$ref": "shapes/group.json"
          },
          {
            "$ref": "shapes/round.json"
          },
          {
            "$ref": "shapes/repeater.json"
          }
        ],
        "type": "object"
      },
      "type": "array"
    },
    "it": {
      "title": "Items",
      "description": "形状项目列表",
      "items": {
        "oneOf": [
          {
            "$ref": "shapes/shape.json"
          },
          {
            "$ref": "shapes/rect.json"
          },
          {
            "$ref": "shapes/ellipse.json"
          },
          {
            "$ref": "shapes/star.json"
          },
          {
            "$ref": "shapes/fill.json"
          },
          {
            "$ref": "shapes/gFill.json"
          },
          {
            "$ref": "shapes/gStroke.json"
          },
          {
            "$ref": "shapes/stroke.json"
          },
          {
            "$ref": "shapes/merge.json"
          },
          {
            "$ref": "shapes/trim.json"
          },
          {
            "$ref": "shapes/group.json"
          },
          {
            "$ref": "shapes/round.json"
          },
          {
            "$ref": "shapes/repeater.json"
          }
        ],
        "type": "object"
      },
      "type": "array"
    }
  }
}
